            "internal": false,
            "bucket": "xxxxxx",
            "folder": "xxxxxx",
            "accessKeyId": "xxxxxx",
            "secretAccessKey": "xxxxxx"
        },
        "aws": {
            "region": "xxxxxx",
            "bucket": "xxxxxx",
            "folder": "xxxxxx",
            "accessKeyId": "xxxxxx",
            "secretAccessKey": "xxxxxx"
        }
    },
    // 效率统计
    "instances_stat": {
        "space": "Af8eM6mAHo7wMDqD3",
